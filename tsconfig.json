{
    "compilerOptions": {
        "module": "esNext",
        "target": "esNext",
        "allowJs": true,
        "skipLibCheck": false,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "sourceMap": false,
        "jsx": "react",
        "allowSyntheticDefaultImports": true
    },
    "exclude": ["./node_modules"],
    "paths":{
        "express": ["./node_modules/express/lib/express"]
    },
    "include": ["src"]
}